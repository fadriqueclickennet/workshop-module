{
  "name": "fadriqueclickennet/workshop-module",
  "type": "clickennet-module",
  "description": "Workshop module for ClickennetCMS. Handles the activation of modules.",
  "keywords": [
    "clickennetcms",
    "workshop",
    "modules"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "fadriqueclickennet",
      "email": "fadriqueclickennet@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "composer/installers": "~1.0",
    "symfony/yaml": "^5.0.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.5",
    "orchestra/testbench": "3.8.*",
    "spatie/phpunit-snapshot-assertions": "^1.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Workshop\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
